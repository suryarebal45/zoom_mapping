      <!-- Calculated Fields for KPIs -->
      <column caption='Monthly Recurring Revenue (MRR)' datatype='real' name='[Calculation_MRR]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='SUM(IF DATEPART(&apos;month&apos;,[EVENT_DATE]) = DATEPART(&apos;month&apos;,TODAY()) THEN [AMOUNT] ELSE 0 END)' />
      </column>
      <column caption='Revenue by Plan Type' datatype='real' name='[Calculation_Revenue_By_Plan]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='SUM([AMOUNT])' />
      </column>
      <column caption='License Utilization Rate' datatype='real' name='[Calculation_License_Utilization]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='COUNTD([USER_ID]) / AVG([MAXIMUM_USER_LIMIT])' />
      </column>
      <column caption='License Expiration Trends' datatype='integer' name='[Calculation_License_Expiration]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='COUNT(IF [CONTRACT_END_DATE] &lt; DATEADD(&apos;month&apos;,3,TODAY()) THEN 1 END)' />
      </column>
      <column caption='Usage Correlation with Billing' datatype='real' name='[Calculation_Usage_Billing_Correlation]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='CORR([DURATION_MINUTES],[AMOUNT])' />
      </column>
      <column caption='Total Revenue' datatype='real' name='[Calculation_Total_Revenue]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='SUM([AMOUNT])' />
      </column>
      <column caption='Churn Rate' datatype='real' name='[Calculation_Churn_Rate]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='COUNT(IF [CONTRACT_END_DATE] &lt; TODAY() THEN 1 END) / COUNTD([USER_ID])' />
      </column>
      <column caption='Email Masked' datatype='string' name='[Calculation_Email_Masked]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='LEFT([EMAIL_ADDRESS],1)+&quot;****&quot;+RIGHT([EMAIL_ADDRESS],FIND(&quot;@&quot;,[EMAIL_ADDRESS])-1)' />
      </column>
      <column caption='User Name Masked' datatype='string' name='[Calculation_User_Name_Masked]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='LEFT([USER_NAME],1)+&quot;****&quot;' />
      </column>
      
